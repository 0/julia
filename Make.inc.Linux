CC = gcc
CXX = g++
CFLAGS = 

HFILEDIRS = /usr/local/include
LIBDIRS = /usr/local/lib
OSLIBS = -Wl,--export-dynamic
EFENCE =

ifeq ($(NBITS),32)
CONFIG = -DLINUX -DARCH_X86 -D__CPU__=586 -DUSE_READLINE -DBOEHM_GC
endif
ifeq ($(NBITS),64)
CONFIG = -DLINUX -DARCH_X86_64 -D__CPU__=586 -DBITS64 -DUSE_READLINE -DBOEHM_GC
endif
