CC = gcc
CXX = g++
CFLAGS = 

GAMBIT = /usr/local/Gambit-C
GAMBITGSC = gsc
GAMBITGSI = gsi
GAMBITLIB = $(GAMBIT)/lib/libgambc.a
HFILEDIRS = /usr/local/include $(GAMBIT)/include
LIBDIRS = /usr/local/lib
OSLIBS =
EFENCE =

NBITS = 32
ifeq ($(NBITS),32)
CONFIG = -DLINUX -DARCH_X86 -D__CPU__=586 -DUSE_READLINE
endif
ifeq ($(NBITS),64)
CONFIG = -DLINUX -DARCH_X86_64 -D__CPU__=586 -DBITS64 -DUSE_READLINE
endif
