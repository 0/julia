digraph Numbers {
  ordering = out;
  node [shape=none,width=0.9]

  subgraph RealComplex {
    rank = same;
    Real
    Complex [label="Complex{T<:Real}",width=10]
  }

  subgraph Reals {
    rank = same;
    node [width=1.25]
    Int
    Float
    Rational [label="Rational{T<:Int}",width=2.5]
  }

  Any -> Number

  Number -> Real
  Number -> Complex

  Real -> Int -> Uint
  Real -> Float
  Real -> Rational
}
